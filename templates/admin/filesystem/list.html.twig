<ul class="jqueryFileTree" style="display: none;">
	{% for file in files %}
		{% if file.type == 'directory' %}
			<li class="directory collapsed">
				<a href="#" rel="{{ file.path }}">{{ file.name }}</a>
			</li>
		{% else %}
			<li class="file ext_{{file.extension}}">
				<a href="#" rel="{{ file.path }}">{{ file.name }}</a>
			</li>
		{% endif %}
	{% endfor %}
</ul>
